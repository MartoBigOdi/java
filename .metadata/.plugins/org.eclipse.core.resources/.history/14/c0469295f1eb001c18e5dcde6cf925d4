package excep.TDAS;

public class Zona {
	
	private String nombre;
	private int codigo;
	private double superficie;
	
	public Zona(String nombre, int cpdigp, double superficie) {
		this.nombre = nombre;
		this.codigo = cpdigp;
		this.superficie = superficie;
	}
	public String getNombre() {
		return nombre;
	}
	public int getCpdigp() {
		return codigo;
	}
	public double getSuperficie() {
		return superficie;
	}
	
	
	

}
